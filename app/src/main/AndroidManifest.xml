<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.cs125.shubhjason.omnicalc">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Options" />
        <activity
            android:name=".StandardCalc"
            android:label="@string/title_activity_standard_calc"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AlgebraCalc" />
        <activity android:name=".GraphingCalc" />
        <activity android:name=".infoforoptions"
            android:theme="@style/AppTheme.Custometheme1"
            ></activity>
    </application>

</manifest>